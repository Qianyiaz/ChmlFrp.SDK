<Project>

    <PropertyGroup>
        <Version>2.0.7.2</Version>
        <Nullable>enable</Nullable>
        <LangVersion>preview</LangVersion>
        <ImplicitUsings>true</ImplicitUsings>
        <IsAotCompatible>true</IsAotCompatible>
        <TargetFramework>net8.0</TargetFramework>
        <PackageReadmeFile>readme.md</PackageReadmeFile>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageLicenseExpression>MPL-2.0</PackageLicenseExpression>
        <PackageIconUrl>https://chmlfrp.cn/favicon.ico</PackageIconUrl>
        <RepositoryUrl>https://github.com/Qianyiaz/ChmlFrp.SDK</RepositoryUrl>
        <PackageTags>chmlfrp;http;json;aot;trimmable;cross-platform;dotnet</PackageTags>
    </PropertyGroup>

</Project>